<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Image Text"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_list_v4">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <styletab
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/mnt/overlay/cq/gui/components/authoring/dialog/style/tab_design/styletab"/>
                    <allowedcomponents
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Allowed Components"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <components
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/allowedcomponents"
                                name="./components"/>
                        </items>
                    </allowedcomponents>
                    <plugins
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Plugins"
                        sling:resourceType="granite/ui/components/coral/foundation/accordion"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <Features
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Features"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <fixedcol
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        maximized="{Boolean}false">
                                        <items jcr:primaryType="nt:unstructured">
                                            <col1
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <edit
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <paste-plaintext
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show paste plain text option"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                                name="paste-plaintext"/>
                                                            <paste-wordhtml
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show paste from word option"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                                name="paste-wordhtml"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            name="edit"
                                                            plugin-root-name="rtePlugins"/>
                                                    </edit>
                                                    <find-replace
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <find-replace
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show find &amp; replace options"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                                features="*"
                                                                name="findreplace"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            plugin-root-name="rtePlugins"/>
                                                    </find-replace>
                                                    <undo-redo
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <undo
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show undo &amp; redo options"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                                features="*"
                                                                name="undo"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            plugin-root-name="rtePlugins"/>
                                                    </undo-redo>
                                                    <spellcheck
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <spellcheck
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show spell checker options"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                                features="*"
                                                                name="spellcheck"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            plugin-root-name="rtePlugins"/>
                                                    </spellcheck>
                                                    <image
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <image
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show inserted image modification options"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                                features="*"
                                                                name="image"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            plugin-root-name="rtePlugins"/>
                                                    </image>
                                                    <editHTML
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <editHTML
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show HTML source editing option"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                                name="sourceedit"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            name="misctools"
                                                            plugin-root-name="rtePlugins"/>
                                                    </editHTML>
                                                </items>
                                            </col1>
                                        </items>
                                    </fixedcol>
                                </items>
                            </Features>
                            <Formatting
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Formatting"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <fixedcol
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        maximized="{Boolean}false">
                                        <items jcr:primaryType="nt:unstructured">
                                            <col1
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <tables
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <tables
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show table options"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                                features="*"
                                                                name="table"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            plugin-root-name="rtePlugins"/>
                                                    </tables>
                                                    <lists
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <lists
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show list options (bullet, number, indent, unindent)"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                                defaultEnabled="{Boolean}true"
                                                                features="*"
                                                                name="lists"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            plugin-root-name="rtePlugins"/>
                                                    </lists>
                                                    <align
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <align
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show custom alignment options (left aligned, centered, right aligned)"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                                features="*"
                                                                name="justify"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            plugin-root-name="rtePlugins"/>
                                                    </align>
                                                    <format
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <bold
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show bold option"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                                defaultEnabled="{Boolean}true"
                                                                name="bold"/>
                                                            <italic
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show italic option"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                                defaultEnabled="{Boolean}true"
                                                                name="italic"/>
                                                            <underline
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show underline option"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                                                name="underline"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            name="format"
                                                            plugin-root-name="rtePlugins"/>
                                                    </format>
                                                    <links
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <links
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show link insertion options (link, unlink, anchor)"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                                defaultEnabled="{Boolean}true"
                                                                features="modifylink,unlink,anchor"
                                                                name="links"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            plugin-root-name="rtePlugins"/>
                                                    </links>
                                                    <subsuperscript
                                                        granite:class="js-cq-IPEPlugin-group"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <subsuperscript
                                                                jcr:primaryType="nt:unstructured"
                                                                jcr:title="Show superscript and subscript options"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                                                features="*"
                                                                name="subsuperscript"/>
                                                        </items>
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            plugin-root-name="rtePlugins"/>
                                                    </subsuperscript>
                                                </items>
                                            </col1>
                                        </items>
                                    </fixedcol>
                                </items>
                            </Formatting>
                            <Paraformat
                                granite:class="js-cq-IPEPlugin-group"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Paragraph Styles"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <paraformat
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Enable paragraph styles"
                                        sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/plugin"
                                        features="*"
                                        name="paraformat"/>
                                    <configWrapper
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/wrapper"
                                        configPath="./rtePlugins/paraformat/formats">
                                        <formats
                                            granite:class="cq-paragraphstyles"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                            composite="{Boolean}true"
                                            fieldLabel="Custom Formats">
                                            <field
                                                granite:class="cq-paragraphstyles-field"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                name="./rtePlugins/paraformat/formats">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <tag
                                                        granite:class="cq-rte-symbol"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        emptyText="Element"
                                                        name="tag"/>
                                                    <description
                                                        granite:class="cq-rte-symbol-description"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        emptyText="Display text"
                                                        name="description"/>
                                                </items>
                                            </field>
                                        </formats>
                                    </configWrapper>
                                    <mergeOverride
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                        name="./rtePlugins/paraformat/formats/override"
                                        value="{Boolean}true"/>
                                </items>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    plugin-root-name="rtePlugins"/>
                            </Paraformat>
                            <Characters
                                granite:class="js-cq-IPEPlugin-group"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Special Characters"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <specialchars
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Enable special characters"
                                        sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/pluginfeature"
                                        name="specialchars"/>
                                    <configWrapper
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/inplaceediting/configuration/wrapper"
                                        configPath="./rtePlugins/misctools/specialCharsConfig/chars">
                                        <specialCharsConfig
                                            granite:class="cq-specialcharacters"
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                            composite="{Boolean}true"
                                            fieldLabel="Add Characters">
                                            <field
                                                granite:class="cq-specialcharacters-field"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                name="./rtePlugins/misctools/specialCharsConfig/chars">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <entity
                                                        granite:class="cq-rte-symbol"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        emptyText="Entity"
                                                        name="entity"/>
                                                    <name
                                                        granite:class="cq-rte-symbol-description"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        emptyText="Display text"
                                                        name="name"/>
                                                </items>
                                            </field>
                                        </specialCharsConfig>
                                    </configWrapper>
                                    <mergeOverride
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                        name="./rtePlugins/misctools/specialCharsConfig/chars/override"
                                        value="{Boolean}true"/>
                                    <items jcr:primaryType="nt:unstructured"/>
                                </items>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    name="misctools"
                                    plugin-root-name="rtePlugins"/>
                            </Characters>
                        </items>
                    </plugins>
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Main"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <content
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <enableDmFeatures
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="When checked, Dynamic Media features are enabled."
                                        name="./enableDmFeatures"
                                        text="Enable DM features"
                                        uncheckedValue="false"
                                        value="{Boolean}true">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/feature"
                                            feature="com.adobe.dam.asset.scene7.feature.flag"/>
                                    </enableDmFeatures>
                                    <enableAssetDelivery
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="When checked, Web Optimized Image Delivery is enabled."
                                        name="./enableAssetDelivery"
                                        text="Enable Web Optimized Images"
                                        uncheckedValue="false"
                                        value="{Boolean}true">
                                        <granite:rendercondition
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="core/wcm/components/rendercondition/isAssetDeliveryEnabled"/>
                                    </enableAssetDelivery>
                                    <disableLazyLoading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="{Boolean}false"
                                        fieldDescription="When checked, it will preload all images, without using lazy loading."
                                        name="./disableLazyLoading"
                                        text="Disable lazy loading"
                                        uncheckedValue="false"
                                        value="{Boolean}true"/>
                                    <decorative
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Check if the image should be ignored by assistive technology and therefore does not require an alternative text. This applies to decorative images only."
                                        name="./isDecorative"
                                        text="Image is decorative"
                                        uncheckedValue="false"
                                        value="{Boolean}true"/>
                                    <altValueFromDAM
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="{Boolean}true"
                                        fieldDescription="When checked, populate the image's alt attribute with the value of the dc:description metadata in DAM."
                                        name="./altValueFromDAM"
                                        text="Get alternative text from DAM"
                                        uncheckedValue="false"
                                        value="{Boolean}true"/>
                                    <titleValueFromDAM
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="{Boolean}true"
                                        fieldDescription="When checked, populate the image's caption with the value of the dc:title metadata in DAM."
                                        name="./titleValueFromDAM"
                                        text="Get caption from DAM"
                                        uncheckedValue="false"
                                        value="{Boolean}true"/>
                                    <displayCaptionPopup
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="{Boolean}true"
                                        fieldDescription="When checked, the caption won't be displayed below the image, but as a pop-up displayed by some browsers when hovering over the image."
                                        name="./displayPopupTitle"
                                        text="Display caption as pop-up"
                                        uncheckedValue="false"
                                        value="{Boolean}true"/>
                                    <resizeWidth
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue="{Long}1280"
                                        fieldDescription="Define a resize width (in pixels). Its value will be used for resizing the width of the base images that are DAM assets. The aspect ratio of the images will be preserved. If the value is bigger than the natural width of an image, it will have no effect. Note that this setting has no effect on SVG images."
                                        fieldLabel="Resize Width"
                                        max="{Long}3840"
                                        min="{Long}1"
                                        name="./resizeWidth"
                                        required="{Boolean}false"
                                        typeHint="Long"/>
                                    <heading
                                        granite:class="coral-Heading coral-Heading--4"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/heading"
                                        level="{Long}4"
                                        text="Responsive Image Settings"/>
                                    <responsiveGroup
                                        granite:class="cmp-image__editor-sizes"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                        <items jcr:primaryType="nt:unstructured">
                                            <widths
                                                granite:class="core-image-widths"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldDescription="Define a list of alternative image widths (in pixels). The most appropriate image will be loaded by the component. Note that this setting has no effect on SVG images."
                                                fieldLabel="Widths">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                    defaultValue="{Long}1280"
                                                    min="{Long}0"
                                                    name="./allowedRenditionWidths"
                                                    required="{Boolean}true"/>
                                            </widths>
                                            <sizes
                                                granite:class="core-image-widths"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldDescription="Define a list of media conditions which indicates what image width should be use"
                                                fieldLabel="Sizes">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    name="./sizes"/>
                                            </sizes>
                                        </items>
                                    </responsiveGroup>
                                    <jpegQuality
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue="{Long}82"
                                        fieldDescription="The quality factor (in percentage from 0 and 100) for transformed (e.g. scaled or cropped) JPEG images."
                                        fieldLabel="JPEG Quality"
                                        max="{Long}100"
                                        min="{Long}0"
                                        name="./jpegQuality"
                                        required="{Boolean}false"
                                        typeHint="Long"/>
                                </items>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    cmp-image-v3-dialog-policy-hook="imageProperties"/>
                            </content>
                        </items>
                    </properties>
